<install version="1.5" type="plugin" group="editors" method="upgrade">
	<name>Editor - CKEditor</name>
	<version>1.2</version>
	<creationDate>September 2009</creationDate>
	<author>CMSSpace</author>
	<authorurl>http://www.cmsspace.com</authorurl>
	<license>GPL</license>
	<description>CKEditor is a platform independent web based JavaScript HTML WYSIWYG Editor control.</description>
	<files>
	<filename plugin="ckeditor">ckeditor.php</filename>
	<folder>ckeditor</folder>
	<folder>AjexFileManager</folder>
</files>
	<languages>
		<language tag="en-GB">language/en-GB.plg_editors_ckeditor.ini</language>
	</languages>
	<params>
		<param name="language" type="list" default="en" label="Language" description="">
			<option value="af">Afrikaans</option><option value="ar">Arabic</option><option value="eu">Basque</option><option value="bn">Bengali/Bangla</option><option value="bs">Bosnian</option><option value="bg">Bulgarian</option><option value="ca">Catalan</option><option value="zh-cn">Chinese Simplified</option><option value="zh">Chinese Traditional</option><option value="hr">Croatian</option><option value="cs">Czech</option><option value="da">Danish</option><option value="nl">Dutch</option><option value="en">English</option><option value="en-au">English (Australia)</option><option value="en-ca">English (Canadian)</option><option value="en-uk">English (United Kingdom)</option><option value="eo">Esperanto</option><option value="et">Estonian</option><option value="fo">Faroese</option><option value="fi">Finnish</option><option value="fr">French</option><option value="fr-ca">French (Canada)</option><option value="gl">Galician</option><option value="de">German</option><option value="el">Greek</option><option value="gu">Gujarati</option><option value="he">Hebrew</option><option value="hi">Hindi</option><option value="hu">Hungarian</option><option value="is">Icelandic</option><option value="it">Italian</option><option value="ja">Japanese</option><option value="km">Khmer</option><option value="ko">Korean</option><option value="lv">Latvian</option><option value="lt">Lithuanian</option><option value="ms">Malay</option><option value="mn">Mongolian</option><option value="no">Norwegian</option><option value="nb">Norwegian Bokmal</option><option value="fa">Persian</option><option value="pl">Polish</option><option value="pt-br">Portuguese (Brazil)</option><option value="pt">Portuguese (Portugal)</option><option value="ro">Romanian</option><option value="ru">Russian</option><option value="sr">Serbian (Cyrillic)</option><option value="sr-latn">Serbian (Latin)</option><option value="sk">Slovak</option><option value="sl">Slovenian</option><option value="es">Spanish</option><option value="sv">Swedish</option><option value="th">Thai</option><option value="tr">Turkish</option><option value="uk">Ukrainian</option><option value="vi">Vietnamese</option>
		</param>
		<param name="toolbar" type="list" default="Default" label="Administrator End Toolbar Mode" description="">
			<option value="Default">Default</option>
			<option value="Creative">Creative</option>
			<option value="Blog">Blog</option>
		</param>
		<param name="toolbar_ft" type="list" default="Default" label="FrontEnd Toolbar Mode" description="">
			<option value="Default">Default</option>
			<option value="Creative">Creative</option>
			<option value="Blog">Blog</option>
		</param>
		<param name="Creative_ToolBar"  default="[ 'Bold', 'Italic', '-', 'NumberedList', 'BulletedList', '-', 'Link', 'Unlink' ]" type="textarea" cols="30" rows="5" label="Creative ToolBar" description="" />
		<param name="Blog_ToolBar"  default="[ 'Bold', 'Italic', '-', 'NumberedList', 'BulletedList', '-', 'Link', 'Unlink' ]" type="textarea" cols="30" rows="5" label="Blog ToolBar" description="" />
		<param name="skin" type="list" default="kama" label="Skin" description="EDITORSKIN">
			<option value="kama">kama</option>
			<option value="office2003">office2003</option>
			<option value="v2">v2</option>
		</param>
		<param name="Color" type="text" default="#6B6868" label="Color" description="" />
		<param name="entermode" type="list" default="1" label="Enter Mode" description="">
			<option value="0">P Elements</option>
			<option value="1">BR Elements</option>
			<option value="2">Div Elements</option>
		</param>
		<param name="shiftentermode" type="list" default="0" label="Shift Enter Mode" description="">
			<option value="0">P Elements</option>
			<option value="1">BR Elements</option>
			<option value="2">Div Elements</option>
		</param>
 		<param name="ajex" type="radio" default="1" label="Ajex File Manager" description="">
			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
 		<param name="skinfm" type="list" default="light" label="Skin File Manager" description="">
			<option value="light">light</option>
			<option value="dark">dark</option>
		</param>
 		<param name="usergroup_access" multiple="multiple" default="29|18|19|20|21|30|23|24|25" type="usergroup" label="User Group Access file manager" description="" />
		
	</params>
</install>